# <https://trac.macports.org/raw-attachment/ticket/59927/bison-test-results-20210811-95b72.log.xz>
m4_define([AT_CHECK_HTML],
[AT_KEYWORDS([html])
AT_CHECK([sed -e 's|content="text/html; charset=UTF-8"|content="text/html;charset=UTF-8"|g' $1],
          [],
          [$2])])


